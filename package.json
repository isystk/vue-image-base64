{
  "name": "vuejs-image-base64",
  "version": "1.0.1",
  "description": "画像ファイルをBase64に変換するVue.js用コンポーネントです",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy-demo": "gh-pages -d dist",
    "build-bundle": "vue-cli-service build --target lib --name vue-image-base64 ./src/library-main.js"
  },
  "main": "dist/vue-image-base64.common.js",
  "unpkg": "./dist/vue-image-base64.umd.min.js",
  "jsdelivr": "./dist/vue-image-base64.umd.min.js",
  "author": "isystk",
  "license": "MIT",
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "css-loader": "^5.2.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11",
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "dependencies": {
    "heic2any": "0.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
